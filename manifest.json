{
    "name":"JIRA width fix",
    "version":"0.0.3",
    "manifest_version":2,
    "icons": {
        "32": "icons/jira_32px.png",
        "128": "icons/jira_128px.png"
    },
    "browser_action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "js": [ "jquery-3.1.1.min.js", "content.js" ],
            "matches": [ "http://*/*", "https://*/*"]
        }
    ],
    "background": {
        "scripts": ["jquery-3.1.1.min.js", "background.js"]
    }
}